{
  "name": "rg-async",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": ">=7.10.1"
  },
  "description": "Functional utility methods to run async code with promises (async/await keywords)",
  "main": "src/index.js",
  "scripts": {
    "check-eslint": "gulp lint",
    "watch-eslint": "gulp watch-lint",
    "show-docs": "open ./docs/index.html",
    "docs": "gulp docs && npm run show-docs",
    "show-coverage": "open ./coverage/lcov-report/index.html",
    "check-coverage": "istanbul check-coverage",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "delete-coverage": "gulp clean-coverage",
    "test": "istanbul cover _mocha -- -t 10000 --recursive -R spec",
    "test-dev": "npm run test && npm run show-coverage",
    "start": "gulp",
    "commit": "git-cz",
    "build": "gulp build && npm run test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubengomex/rg-async.git"
  },
  "keywords": [
    "Promises",
    "Functional Programming",
    "Utility Functions",
    "ES6",
    "async/await"
  ],
  "files": [
    "src",
    "README.md"
  ],
  "author": "RÃºben Gomes <gomesruben21@gmail.com> (http://rubengomes10.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rubengomex/rg-async/issues"
  },
  "homepage": "https://github.com/rubengomex/rg-async#readme",
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chai": "3.5.0",
    "chai-as-promised": "7.1.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.17.1",
    "ghooks": "2.0.0",
    "gulp": "3.9.1",
    "gulp-beautify": "2.0.1",
    "gulp-eslint": "3.0.1",
    "gulp-jsdoc3": "1.0.1",
    "gulp-nodemon": "2.2.1",
    "gulp-rm": "1.0.2",
    "ink-docstrap": "^1.3.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "run-sequence": "1.2.2",
    "semantic-release": "^6.3.6"
  },
  "dependencies": {
    "lodash": "4.17.10"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
